{
  "rewrites": [
    { "source": "/editor/", "destination": "https://starmap-lp.vercel.app/" },
    {
      "source": "/editor/:match*",
      "destination": "https://starmap-lp.vercel.app/:match*"
    },
    {
      "source": "/",
      "destination": "https://starmap.vercel.app/editor"
    },
    {
      "source":  "/:match*",
      "destination": "https://starmap.vercel.app/editor/:match*"
    }
  ]
}
